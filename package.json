{
  "name": "pdf-data-extractor",
  "version": "1.0.0",
  "description": "AI-powered PDF data extraction tool with local and Box.com integration",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint server/ --ext .js",
    "typecheck": "echo 'No TypeScript files to check'",
    "check-keys": "node check_api_keys.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.1",
    "openai": "^4.28.4",
    "@anthropic-ai/sdk": "^0.24.3",
    "box-node-sdk": "^3.8.0",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "rate-limiter-flexible": "^5.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "@eslint/js": "^9.0.0"
  },
  "keywords": ["pdf", "ai", "data-extraction", "box-com", "react"],
  "author": "Your Name",
  "license": "MIT"
}